{
   "name": "dna.js",
   "version": "1.7.0",
   "description": "An uncomplicated user interface library for building data-driven semantic templates",
   "summary": "dna.js is a lightweight easy-to-use UI library enabling developers to rapidly build maintainable JavaScript applications.",
   "license": "MIT",
   "main": "dist/dna.js",
   "browser": "dist/dna.min.js",
   "style": "dist/dna.css",
   "files": [
      "dist"
   ],
   "repository": "github:dnajs/dna.js",
   "homepage": "https://github.com/dnajs/dna.js",
   "bugs": "https://github.com/dnajs/dna.js/issues",
   "docs": "https://dnajs.org/docs",
   "author": "Center Key (https://centerkey.com)",
   "keywords": [
      "clone",
      "dom",
      "engine",
      "html",
      "jamstack",
      "jquery",
      "json",
      "template",
      "templating",
      "ui"
   ],
   "jshintConfig": {
      "esversion": 9,
      "strict": "implied",
      "eqeqeq": true,
      "singleGroups": true,
      "nonbsp": true,
      "undef": true,
      "unused": true,
      "varstmt": true,
      "browser": true,
      "mocha": true,
      "node": true,
      "globals": {
         "$": true,
         "dna": true,
         "window": true
      }
   },
   "scripts": {
      "pretest": "jshint *.js spec website && gulp build-dist",
      "test": "mocha spec && env specMode=minified mocha spec",
      "posttest": "gulp report-size",
      "interactive": "browser-sync . --startPath spec/visual.html",
      "prewebsite": "gulp clean-website",
      "website": "dnaReleasedVersion=$(git tag | tail -1) gulp build-website",
      "postwebsite": "gulp update-readme && gulp validate-html"
   },
   "cdnVersion": {
      "fetchJson": "2.2",
      "fontAwesome": "5.15",
      "highlightjs": "10.5",
      "hljsEnhance": "0.0",
      "jQuery": "3.5",
      "webIgnition": "1.3"
   },
   "peerDependencies": {
      "jquery": ">=3.5"
   },
   "dependencies": {},
   "devDependencies": {
      "@babel/core": "~7.12",
      "@babel/preset-env": "~7.12",
      "@fortawesome/fontawesome-free": "~5.15",
      "babel-minify": "~0.5",
      "browser-sync": "~2.26",
      "canvas": "~2.6",
      "del": "~6.0",
      "fetch-json": "~2.3",
      "gulp": "~4.0",
      "gulp-append-prepend": "~1.0",
      "gulp-babel": "~8.0",
      "gulp-file-include": "~2.3",
      "gulp-header": "~2.0",
      "gulp-htmlhint": "~4.0",
      "gulp-rename": "~2.0",
      "gulp-replace": "~1.0",
      "gulp-size": "~3.0",
      "gulp-w3c-html-validator": "~2.0",
      "highlight.js": "~10.5",
      "hljs-enhance": "~0.0",
      "jquery": "~3.5",
      "jsdom": "~16.4",
      "jshint": "~2.12",
      "merge-stream": "~2.0",
      "mocha": "~8.2",
      "web-ignition": "~1.3"
   }
}
