{
   "name": "dna.js",
   "version": "1.8.3",
   "description": "An uncomplicated user interface library for building data-driven semantic templates (written in TypeScript)",
   "license": "MIT",
   "type": "module",
   "module": "dist/dna.esm.js",
   "jsdelivr": "dist/dna.min.js",
   "main": "dist/dna.umd.cjs",
   "style": "dist/dna.css",
   "types": "dist/dna.d.ts",
   "files": [
      "dist"
   ],
   "exports": {
      ".": {
         "import": "./dist/dna.esm.js",
         "require": "./dist/dna.umd.cjs"
      },
      "./": "./dist/"
   },
   "repository": "github:dnajs/dna.js",
   "homepage": "https://github.com/dnajs/dna.js",
   "bugs": "https://github.com/dnajs/dna.js/issues",
   "docs": "https://dnajs.org/docs",
   "author": "Center Key (https://centerkey.com)",
   "keywords": [
      "clone",
      "dom",
      "engine",
      "html",
      "jamstack",
      "jquery",
      "json",
      "template",
      "templating",
      "typescript",
      "ui"
   ],
   "meta": {
      "summary": "dna.js is a lightweight easy-to-use UI library enabling developers to rapidly build maintainable JavaScript applications."
   },
   "jshintConfig": {
      "esversion": 10,
      "strict": "implied",
      "eqeqeq": true,
      "singleGroups": true,
      "nonbsp": true,
      "undef": true,
      "unused": true,
      "varstmt": true,
      "browser": true,
      "mocha": true,
      "node": true,
      "globals": {
         "$": true,
         "dna": true,
         "window": true
      }
   },
   "eslintConfig": {
      "ignorePatterns": [
         "build",
         "dist",
         "node_modules"
      ],
      "root": true,
      "parser": "@typescript-eslint/parser",
      "plugins": [
         "@typescript-eslint"
      ],
      "extends": [
         "eslint:recommended",
         "plugin:@typescript-eslint/recommended"
      ],
      "rules": {
         "@typescript-eslint/no-non-null-assertion": "off"
      }
   },
   "scripts": {
      "lint": "       jshint . --exclude node_modules,build,dist website && eslint --max-warnings 0 . --ext .ts",
      "tsc": "        tsc && tsc --module UMD --outDir build/umd",
      "pretest": "    npm run lint && rimraf build dist **/.DS_Store && npm run tsc && gulp make-dist",
      "test": "       w3c-html-validator spec && mocha spec/*.spec.js && gulp report-size",
      "interactive": "browser-sync . --startPath spec/visual.html",
      "website": "    rimraf website-target && dnaReleasedVersion=$(git tag | tail -1) gulp build-website",
      "postwebsite": "gulp update-readme && w3c-html-validator docs website-target && browser-sync website-target"
   },
   "cdnVersion": {
      "fetchJson": "2.5",
      "fontAwesome": "5.15",
      "highlightjs": "11.2",
      "hljsEnhance": "1.0",
      "jQuery": "3.6",
      "webIgnition": "1.4"
   },
   "dependencies": {},
   "peerDependencies": {
      "jquery": ">=3.5"
   },
   "devDependencies": {
      "@babel/core": "~7.14",
      "@babel/preset-env": "~7.14",
      "@fortawesome/fontawesome-free": "~5.15",
      "@types/jquery": "~3.5",
      "@types/jsdom": "~16.2",
      "@types/node": "~16.4",
      "@typescript-eslint/eslint-plugin": "~4.28",
      "@typescript-eslint/parser": "~4.28",
      "assert-deep-strict-equal": "~0.0",
      "babel-minify": "~0.5",
      "browser-sync": "~2.27",
      "canvas": "~2.8",
      "eslint": "~7.32",
      "fetch-json": "~2.5",
      "gulp": "~4.0",
      "gulp-append-prepend": "~1.0",
      "gulp-babel": "~8.0",
      "gulp-file-include": "~2.3",
      "gulp-header": "~2.0",
      "gulp-rename": "~2.0",
      "gulp-replace": "~1.1",
      "gulp-size": "~4.0",
      "highlight.js": "~11.2",
      "hljs-enhance": "~1.0",
      "jquery": "~3.6",
      "jsdom": "~16.7",
      "jshint": "~2.13",
      "merge-stream": "~2.0",
      "mocha": "~9.0",
      "puppeteer": "~10.1",
      "puppeteer-browser-ready": "~0.3",
      "rimraf": "~3.0",
      "typescript": "~4.3",
      "w3c-html-validator": "~0.7",
      "web-ignition": "~1.4"
   }
}
