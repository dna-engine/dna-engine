{
   "name": "dna.js",
   "version": "1.9.8",
   "description": "An uncomplicated user interface library for building data-driven semantic templates (written in TypeScript)",
   "license": "MIT",
   "type": "module",
   "module": "dist/dna.js",
   "jsdelivr": "dist/dna.min.js",
   "main": "dist/dna.umd.cjs",
   "style": "dist/dna.css",
   "types": "dist/dna.d.ts",
   "files": [
      "dist"
   ],
   "exports": {
      ".": {
         "import": "./dist/dna.js",
         "require": "./dist/dna.umd.cjs"
      },
      "./": "./dist/"
   },
   "repository": "github:dnajs/dna.js",
   "homepage": "https://dnajs.org",
   "bugs": "https://github.com/dnajs/dna.js/issues",
   "docs": "https://dnajs.org/docs",
   "author": "Center Key (https://centerkey.com)",
   "keywords": [
      "clone",
      "dom",
      "engine",
      "html",
      "jamstack",
      "jquery",
      "json",
      "template",
      "templating",
      "typescript",
      "ui"
   ],
   "meta": {
      "summary": "dna.js is a lightweight easy-to-use UI library enabling developers to rapidly build maintainable JavaScript applications."
   },
   "jshintConfig": {
      "esversion": 10,
      "strict": "implied",
      "eqeqeq": true,
      "singleGroups": true,
      "nonbsp": true,
      "undef": true,
      "unused": true,
      "varstmt": true,
      "browser": true,
      "mocha": true,
      "node": true,
      "globals": {
         "$": true,
         "dna": true,
         "window": true
      }
   },
   "eslintConfig": {
      "ignorePatterns": [
         "build",
         "dist",
         "node_modules"
      ],
      "root": true,
      "parser": "@typescript-eslint/parser",
      "plugins": [
         "@typescript-eslint"
      ],
      "extends": [
         "eslint:recommended",
         "plugin:@typescript-eslint/recommended"
      ],
      "rules": {
         "@typescript-eslint/no-non-null-assertion": "off"
      }
   },
   "scripts": {
      "step:01": "rimraf build dist **/.DS_Store",
      "step:02": "jshint . --exclude node_modules,build,dist",
      "step:03": "eslint --max-warnings 0 src --ext .ts",
      "step:04": "tsc",
      "step:05": "tsc --module UMD --outDir build/umd",
      "step:06": "cpy build/umd/dna.js build --rename=dna.umd.cjs --flat=true",
      "step:07": "cpy src/dna.css build --flat=true",
      "step:08": "cpy website/static/panel-nav.css build --flat=true",
      "step:09": "gulp minify-js",
      "step:10": "add-dist-header build dist",
      "step:11": "cpy spec/visual.html spec --rename=visual-min.html --flat=true",
      "step:12": "replace-in-file /dist/dna.dev.js /dist/dna.min.js spec/visual-min.html",
      "step:13": "w3c-html-validator spec",
      "pretest": "npm-run-all step:*",
      "dev:01": "tsc --watch",
      "dev:02": "chokidar 'src/dna.ts' --command 'src/eslint dna.ts'",
      "dev:03": "chokidar 'build/dna.js' --command 'gulp minify-js'",
      "dev:04": "browser-sync . --startPath spec/visual.html --files dist spec",
      "web:01": "rimraf website-target",
      "web:02": "dnaReleasedVersion=$(git tag | tail -1) gulp build-website",
      "web:03": "w3c-html-validator website-target",
      "web:04": "cpy website-target/project.html docs --rename=index.html --flat=true",
      "web:05": "gulp update-readme",
      "test": "mocha spec/*.spec.js",
      "dev": "npm test && npm-run-all --parallel dev:*",
      "build-website": "npm-run-all web:*"
   },
   "cdnVersion": {
      "fetchJson": "2.6",
      "fontAwesome": "6.1",
      "highlightjs": "11.5",
      "hljsEnhance": "1.0",
      "jQuery": "3.6",
      "webIgnition": "1.5"
   },
   "dependencies": {},
   "peerDependencies": {
      "jquery": ">=3.5"
   },
   "devDependencies": {
      "@babel/core": "~7.17",
      "@babel/preset-env": "~7.16",
      "@fortawesome/fontawesome-free": "~6.1",
      "@types/jquery": "~3.5",
      "@types/jsdom": "~16.2",
      "@types/node": "~17.0",
      "@typescript-eslint/eslint-plugin": "~5.15",
      "@typescript-eslint/parser": "~5.15",
      "add-dist-header": "~0.1",
      "assert-deep-strict-equal": "~1.0",
      "babel-minify": "~0.5",
      "browser-sync": "~2.27",
      "canvas": "~2.9",
      "chokidar-cli": "~3.0",
      "cpy-cli": "~4.1",
      "eslint": "~8.11",
      "fetch-json": "~2.6",
      "gulp": "~4.0",
      "gulp-append-prepend": "~1.0",
      "gulp-babel": "~8.0",
      "gulp-file-include": "~2.3",
      "gulp-rename": "~2.0",
      "gulp-replace": "~1.1",
      "gulp-size": "~4.0",
      "highlight.js": "~11.5",
      "hljs-enhance": "~1.0",
      "jquery": "~3.6",
      "jsdom": "~19.0",
      "jshint": "~2.13",
      "merge-stream": "~2.0",
      "mocha": "~9.2",
      "npm-run-all2": "~5.0",
      "puppeteer": "~13.5",
      "puppeteer-browser-ready": "~0.4",
      "replace-in-file": "~6.3",
      "rimraf": "~3.0",
      "serve": "~13.0",
      "typescript": "~4.6",
      "w3c-html-validator": "~1.0",
      "web-ignition": "~1.5"
   }
}
